{
  "name": "webpack-next-externals",
  "version": "0.0.1",
  "description": "Flexible webpack externals configuration for Next.js",
  "main": "index.js",
  "repository": "https://github.com/FormidableLabs/webpack-next-externals",
  "author": "Ryan Roemer <ryan.roemer@formidable.com>",
  "license": "MIT",
  "scripts": {
    "clean": "rimraf \"test/e2e/scenarios/*/.next\"",
    "lint": "eslint .",
    "test:e2e": "mocha \"test/e2e/scenarios/*/*.spec.js\"",
    "test:e2e-cov": "nyc yarn test:e2e",
    "test:spec": "mocha \"test/spec/*/*.spec.js\"",
    "test:spec-cov": "nyc yarn test:spec",
    "test": "yarn test:spec && yarn test:e2e",
    "test-cov": "yarn test:spec-cov && yarn test:e2e-cov",
    "check": "yarn lint && yarn test",
    "check-ci": "yarn lint && yarn test-cov"
  },
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "chai": "^4.3.0",
    "codecov": "^3.8.1",
    "eslint": "^7.19.0",
    "eslint-config-formidable": "^4.0.0",
    "eslint-plugin-filenames": "^1.3.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-promise": "^4.3.1",
    "eslint-plugin-react": "^7.22.0",
    "fs-extra": "^9.1.0",
    "mocha": "^8.3.0",
    "next": "file:test/packages/next",
    "next09": "file:test/packages/next09",
    "next10": "file:test/packages/next10",
    "nyc": "^15.1.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "rimraf": "^3.0.2",
    "sinon": "^9.2.4"
  }
}
